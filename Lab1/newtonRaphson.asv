function xOut = newtonRaphson(polinomio,iteraciones,error,x0)
i = 0;
e = error + 1;
xa = x0;

while i < iteraciones && e > error
    eva = polinomio(xa);
    der = derivada(polinomio,xa);
    xs = xa - (eva) / (der);
    e = abs(xs - xa);
    xa = xs;
    i = i + 1;
end
xOut = xs;
%Se adapta el resultado para mostrarlo luego

end